                               asp-route-direct_material_cost ="@ViewBag.direct_material_cost"
                               asp-route-direct_material_cost_percentage ="@ViewBag.direct_material_cost_percentage"
                               asp-route-sub_material_percentage ="@ViewBag.sub_material_percentage"
                               asp-route-sub_material_cost ="@ViewBag.sub_material_cost"
                               asp-route-sub_material_cost_percentage ="@ViewBag.sub_material_cost_percentage"
                               asp-route-direct_process_cost ="@ViewBag.direct_process_cost"
                               asp-route-direct_process_cost_percentage ="@ViewBag.direct_process_cost_percentage"
                               asp-route-total_direct_cost ="@ViewBag.total_direct_cost"
